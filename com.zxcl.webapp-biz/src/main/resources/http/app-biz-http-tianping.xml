<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="  
	 http://cxf.apache.org/jaxws  http://cxf.apache.org/schemas/jaxws.xsd
     http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd  
     http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd" >

	<!-- 太平报价接口 -->
    <bean id="tianpingHttpQuoteService" class="org.springframework.remoting.httpinvoker.HttpInvokerProxyFactoryBean"> 
    	<property name="serviceUrl" value="${tianping_http_url}" /> 
    	<property name="serviceInterface" value="com.zxcl.tianping_http.api.TianpingHttpQuoteService" /> 
	</bean>
	
	<!-- 太平保单解析接口 -->
    <bean id="taipingHttpAction" class="org.springframework.remoting.httpinvoker.HttpInvokerProxyFactoryBean"> 
        <property name="serviceUrl" value="${share.url}/taiping_http/remoting/TPIntfAction" /> 
        <property name="serviceInterface" value="com.bxtx.tp.intf.biz.action.TPIntfAction" /> 
    </bean>
</beans>